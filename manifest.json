{
    "name": "Geo Search",
    "description": "Googling products supliers from different locations is made easy",
    "version": "1.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/favicon/favicon-16x16.png",
        "32": "icons/favicon/favicon-32x32.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "webRequest"
    ],
    "background": {}
}